{
    "files.associations": {
        "*.ejs": "html",
        "*.ts": "typescript",
        "*.cpp": "cpp",
        "*.h": "cpp",
        "iostream": "cpp"
    },
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },

}